      <Button mnemonicParsing="false" onAction="#onAnnuler" text="Annuler">
         <FlowPane.margin>
            <Insets right="15.0" />
         </FlowPane.margin></Button>
      <TextField fx:id="txtFieldRecherche" onAction="#onRecherche" prefHeight="26.0" prefWidth="336.0" promptText="Rechercher">
